{
  "name": "tic-tac-two",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --watch 'server.ts' --exec \"npx ts-node --project tsconfig.server.json -r tsconfig-paths/register server.ts\"",
    "build": "next build && npm run build:server",
    "build:server": "tsc --project tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "lint": "next lint"
  },
  "dependencies": {
    "next": "15.4.5",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/socket.io": "^3.0.2",
    "@types/socket.io-client": "^3.0.0",
    "cross-env": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "nodemon": "^3.1.10",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5"
  }
}
